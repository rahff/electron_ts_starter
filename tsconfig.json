{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    // "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "rootDir": "./",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true 
  },
  "include": ["renderer/**/*.ts", "shared/**/*.ts"],
  "exclude": ["app", "webpack.config.js"]
}
